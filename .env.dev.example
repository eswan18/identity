# Example development environment configuration
# Copy this to .env.dev and fill in your values

# Database connection
ADMIN_DATABASE_URL="postgresql://..."
DATABASE_URL="postgresql://..."

# Server configuration
HTTP_ADDRESS=":8080"
TEMPLATES_DIR="templates"

# JWT Configuration
# Generate a new ECDSA P-256 key with: openssl ecparam -name prime256v1 -genkey -noout
JWT_PRIVATE_KEY="-----BEGIN EC PRIVATE KEY-----
MHcCAQEEI...your-private-key-here...
-----END EC PRIVATE KEY-----"
JWT_ISSUER="http://localhost:8080"
JWT_AUDIENCE="http://localhost:8000"
